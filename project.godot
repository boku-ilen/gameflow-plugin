;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "GameMode",
"language": "GDScript",
"path": "res://addons/gameflow/GameMode.gd"
}, {
"base": "Node",
"class": "GameState",
"language": "GDScript",
"path": "res://addons/gameflow/GameState.gd"
}, {
"base": "Resource",
"class": "GameStateTransition",
"language": "GDScript",
"path": "res://addons/gameflow/GameStateTransition.gd"
} ]
_global_script_class_icons={
"GameMode": "",
"GameState": "",
"GameStateTransition": ""
}

[application]

config/name="gameflow-plugin"
run/main_scene="res://GameModes/DemoGameMode.tscn"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "gameflow" )

[rendering]

environment/default_environment="res://default_env.tres"
